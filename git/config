[push]
	default = current
[credential]
	helper = cache --timeout=3600
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	lg = log --all --decorate --graph --oneline
[merge]
	ff = only
[core]
	editor = vim 
	excludesfile = /home/jared/.config/git/gitignore
[init]
	templatedir = /home/jared/.config/git/templatedir
[rebase]
	autosquash = true
