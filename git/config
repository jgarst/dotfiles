[push]
	default = current
[credential]
	helper = cache --timeout=3600
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	lg = log --all --decorate --graph --oneline
[merge]
	ff = only
	tool = vimdiff
	conflictstyle = diff3
[core]
	editor = vim 
[rebase]
	autosquash = true
[mergetool]
	prompt = false
